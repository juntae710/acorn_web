package ct_0907;

public class Lv1_baby {
	public static void main(String[] args) {
		int answer = 0;
		String[] babbling = { "ayaye", "wooma" };
		for (int i = 0; i < babbling.length; i++) {
			String b = babbling[i];
			b = b.replace("aya", "1");
			b = b.replace("ye", "1");
			b = b.replace("woo", "1");
			b = b.replace("ma", "1");
			b = b.replace("1", "");

			if (b == "") {
				answer++;
			}

		}

	}
}
