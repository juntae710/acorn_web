package ct_0907;

public class Lv1_baby {
public static void main(String[] args) {
	int answer = 0;
	String[] babbling = {"ayaye","wooma"};
	String b= "wooma";
	for(int i=0; i<babbling.length; i++) {
		
	}
	
	b=b.replace("aya", " ");
	b=b.replace("ye", " ");
	b=b.replace("woo", " ");
	b=b.replace("ma", " ");
	if(b==" ") {
		answer++;
	}
}
}
