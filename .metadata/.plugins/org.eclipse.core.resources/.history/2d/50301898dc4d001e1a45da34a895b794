package ct_0907;

public class Lv1_baby {
public static void main(String[] args) {
	int answer = 0;
	String[] babbling = {"ayaye","wooma"};
	String a=babbling(1);
	for(int i=0; i<babbling.length; i++) {
		
		System.out.println());
		b=b.replace("aya", "1");
		System.out.println(b);
		b=b.replace("ye", "1");
		System.out.println(b);
		b=b.replace("woo", "1");
		System.out.println(b);
		b=b.replace("ma", "1");
		System.out.println(b);
		b=b.replace("1", "");
		System.out.println(b);
		
		if(b=="") {
			answer++;
		}
		
	}
	
	
	System.out.println(answer);
}
}
